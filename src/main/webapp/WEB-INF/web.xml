<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
         version="2.5"
         xmlns="http://java.sun.com/xml/ns/j2ee"
         xsi:schemaLocation="http://java.sun.com/xml/ns/j2ee http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd">

    <display-name>b2.name</display-name>
    <servlet>
        <servlet-name>upload</servlet-name>
        <servlet-class>edu.scupi.UploadServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>confirm</servlet-name>
        <servlet-class>edu.scupi.ConfirmServlet</servlet-class>
    </servlet>

    <servlet-mapping>
        <servlet-name>upload</servlet-name>
        <url-pattern>/upload</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>confirm</servlet-name>
        <url-pattern>/confirm</url-pattern>
    </servlet-mapping>

</web-app>
